{
  "name": "evo-mark/laravel-impersonate",
  "description": "Laravel Impersonate is a plugin that allows to you to authenticate as your users.",
  "type": "library",
  "keywords": [
    "laravel",
    "package",
    "plugin",
    "impersonate",
    "impersonation",
    "user",
    "auth",
    "laravel-package",
    "laravel-plugin"
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0 | ^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0 | ^11.0",
    "mockery/mockery": "^1.3.3",
    "orchestra/testbench": "^8.0 | ^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Craig Riley",
      "email": "c.riley@evomark.co.uk"
    },
    {
      "name": "Marceau Casals",
      "email": "marceau@casals.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "EvoMark\\Impersonate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "EvoMark\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EvoMark\\Impersonate\\ImpersonateServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
